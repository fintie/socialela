{
    "id": "org.elastos.trinity.dapp.did",
    "version": "1.1.2",
    "version_code": 12,
    "name": "Identity",
    "short_name": "Identity",
    "short_description": "Decentralized User Identity Management",
    "description": "Manage your decentralized identity and choose your personal information to share with others.",
    "start_url": "index.html",
    "type": "app",
    "start_visible": "hide",
    "category": "communication",
    "icons": [
        {
            "src": "assets/images/logo.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "assets/images/logo.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "author": {
        "name": "Trinity Tech Co., Ltd.",
        "email": "contact@trinity-tech.io",
        "website": "https://www.trinity-tech.io"
    },
    "default_locale": "en",
    "plugins": [
        "Clipboard",
        "Device",
        "DIDPlugin",
        "NetworkStatus",
        "File",
        "Camera",
        "HivePlugin",
        "FingerprintPlugin",
        "DIDSessionManagerPlugin",
        "CDVIonicKeyboard",
        "PasswordManagerPlugin",
        "ContactNotifierPlugin"
    ],
    "urls": [
        "https://sentry.io/*"
    ],
    "intent_filters": [
        {
            "action": "credaccess"
        },
        {
            "action": "credimport"
        },
        {
            "action": "credissue"
        },
        {
            "action": "registerapplicationprofile"
        },
        {
            "action": "sign"
        }
    ],
    "background_color": "#4e8ef7",
    "theme_display": "show",
    "theme_color": "#4e8ef7"
}
